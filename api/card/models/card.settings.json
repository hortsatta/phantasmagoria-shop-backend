{
  "kind": "collectionType",
  "collectionName": "cards",
  "info": {
    "name": "Card",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "description": {
      "type": "text"
    },
    "attr": {
      "type": "component",
      "repeatable": false,
      "component": "attributes.attributes"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "coverImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "types": {
      "collection": "type",
      "via": "cards",
      "dominant": true
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "cards"
    },
    "updatedBy": {
      "type": "component",
      "repeatable": true,
      "component": "updated-by.updated-by"
    },
    "category": {
      "via": "cards",
      "model": "category"
    },
    "rarity": {
      "via": "cards",
      "model": "rarity"
    },
    "card_products": {
      "via": "cards",
      "collection": "card-product"
    }
  }
}
